/* 1058 둘 다 거짓일 경우만 참 출력하기
입력 0 1 출력 0
*/
import java.util.*;
import java.io.*;

public class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		if( a == 0 && b== 0)
			System.out.println(1);
		else
			System.out.println(0);
	}
}
