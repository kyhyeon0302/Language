import java.util.*;
import java.io.*;

public class Main{
	public static void main(String[] args) throws IOException {
		Scanner sc =new Scanner(System.in);
		int[][] arr = new int[19][19];
		
		for(int i = 0; i<arr.length; i++) { //들어오는 값 그대로 배열 생성함.
			for(int j = 0; j < arr.length; j++) {
				arr[i][j] = sc.nextInt();
			}
		}// 뒤집기 횟수
		int n = sc.nextInt();
		
		for(int i = 0; i < n; i++) { //뒤집기 횟수 만큼 반복
			
			int x = sc.nextInt();
			for(int j=0; j < arr.length; j++) { //받은 x 값 확인
				if(arr[x-1][j] == 0) {
					arr[x-1][j] = 1;
				} else {
					arr[x-1][j] = 0;
				}				
			}
			int y = sc.nextInt(); 
			for(int k=0; k<arr.length; k++) { //받은 y 값 확인
				if(arr[k][y-1] == 0) {
					arr[k][y-1] = 1;
				} else {
					arr[k][y-1] = 0;
				}
			}
		}
		// 해당 배열 출력하기
		for(int i = 0; i < arr.length; i++) {
			for(int j=0; j<arr.length; j++) {
				System.out.print(arr[i][j] + " ");
			}
			System.out.println();
		}
	}
}
