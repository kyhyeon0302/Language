/* 1035 16진 정수 1개 입력받아서 8진수로 출력하기
입력 f 출력 17
*/

import java.util.*;
import java.io.*;

public class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String b = sc.nextLine();
		int a = Integer.valueOf(b, 16); 
	//n진수에서 10진수로 바꿀 때 Integer클래스내 .valueO 사용
		System.out.println(Integer.toOctalString(a));
	//16진수->10진수->8진수 순서로 변환됨

	}
}
