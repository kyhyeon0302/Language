/* 1069 평가 입력받아 다르게 출력하기.
평가 : 내용 A : best!!! B : good!! C : run! D : slowly~ 나머지 문자들 : what?
switch(변수) {
case 값 : // 변수와 값이 일치하게 되면 해당 case실행문을 돌린다.
	실행문;
	break; //해당 실행문을 작동시킨 이후에 switch문을 종료

default; // 변수와 값이 불일치 하게 되면 default 실행문을 돌린다.
	실행문;
	break;
}



*/

import java.util.*;
import java.io.*;


public class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String res = sc.next();
		
		switch(res.toUpperCase()) { // 모든 문자열을 대문자로 변환
									// toLowerCase 모든 문자열을 소문자로 변환
		case "A" :
			System.out.println("best!!!");
			break;
		case "B" :
			System.out.println("good!!");
			break;
		case "C" :
			System.out.println("run!");
			break;
		case "D" :
			System.out.println("slowly~");
			break;
		default :
			System.out.println("what?");
			break;
					
		}		
	}
		
}
